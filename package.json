    "babel-loader": "^8.0.5",
    "autoprefixer": "^9.5.1",
    "clean-webpack-plugin": "^2.0.1",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "extract-loader": "^3.1.0",
